{
  "apps" : [
  {
    "name"        : "study1234",
    "script"      : "app.js",
    "watch"       : true,
    "instances"  : 1,
    "exec_mode"  : "fork",
    "log_date_format": "YYYY-MM-DD HH:mm Z",
    "log_file": "../logs/study1234.log",
